# =================================
# ENGAJA RAP CLUB - Environment Variables
# =================================

# Application
VITE_APP_TITLE="Engaja Rap Club"
VITE_APP_DESCRIPTION="A primeira plataforma Web3 musical dedicada ao rap brasileiro"
VITE_APP_ENV=development
VITE_APP_VERSION=2.1.0

# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_TIMEOUT=10000

# TON Network
VITE_TON_NETWORK=testnet
VITE_TON_RPC_ENDPOINT=https://testnet.toncenter.com/api/v2/jsonRPC
VITE_TON_API_KEY=your_ton_api_key_here
VITE_BSTR_CONTRACT_ADDRESS=EQA...

# Telegram
VITE_TELEGRAM_BOT_NAME=@EngajaRapClubBot
VITE_TELEGRAM_BOT_USERNAME=EngajaRapClubBot
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Domain
VITE_DOMAIN=localhost:3000
VITE_WEBAPP_URL=http://localhost:3000

# Analytics
VITE_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
VITE_MIXPANEL_TOKEN=your_mixpanel_token

# Feature Flags
VITE_ENABLE_PWA=true
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_STAKING=true
VITE_ENABLE_MARKETPLACE=false

# Debug
VITE_DEBUG_MODE=true
VITE_SHOW_REDUX_DEVTOOLS=true

# Security (Backend - se tiver)
JWT_SECRET=your_jwt_secret_here
ENCRYPTION_KEY=your_encryption_key_here

# Database (Backend - se tiver)
DATABASE_URL=postgresql://user:password@localhost:5432/engaja
REDIS_URL=redis://localhost:6379

# External APIs
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
YOUTUBE_API_KEY=your_youtube_api_key
